{
  "name": "hubgit",
  "version": "0.1.0",
  "description": "Command line app for Github",
  "main": "index.js",
  "bin": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:allanhortle/hubgit.git"
  },
  "bugs": {
    "url": "https://github.com/allanhortle/hubgit/issues"
  },
  "homepage": "https://github.com/allanhortle/hubgit",
  "scripts": {
    "test": "yarn run lint && nyc ava",
    "ava": "ava",
    "build": "rm -rf lib; BABEL_ENV=build babel src -d lib",
    "prepublish": "yarn build"
  },
  "author": "allanhortle",
  "license": "None",
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.6.5",
    "commander": "^2.9.0",
    "immutable": "^3.8.1",
    "marked": "^0.3.6"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-preset-blueflag": "^0.0.1",
    "babel-register": "^6.18.0",
    "eslint": "^3.13.0",
    "eslint-config-blueflag": "^0.4.0",
    "nyc": "^10.0.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "powerAssert": true,
    "files": [
      "src/**/*-test.{js,jsx}"
    ]
  },
  "nyc": {
    "all": true,
    "require": [
      "babel-register"
    ],
    "extension": [
      ".jsx",
      ".js"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "html",
      "text"
    ]
  }
}
